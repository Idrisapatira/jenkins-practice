pipeline {
    agent any

    stages {
        stage('Clone Repo') {
            steps {
                echo 'Code cloned from GitHub.'
            }
        }
        stage('Build') {
            steps {
                echo 'Building the app...'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests...'
            }
        }
    }
}
